<div class="user">
	
<% if @user.id!=current_user.id%>
<%= @user.email%>
<%if !Following.where(:following_id=>@user.id,:follower_id=>current_user.id).first%>
      <%= link_to "Follow", url_for(controller: :home, action: :follow, user_id: @user.id), method: :post %>
    <%else   %>
       <%= link_to "UnFollow", url_for(controller: :home, action: :follow, user_id: @user.id), method: :post %>
<%end%>

<%end%>

<br><br><br><hr>
<h1>User List </h1>
<ol>

<% user=User.all%>
<% user.each do |user|%>
<%if user.id!=current_user.id%>
<li><%= user.email%></li>
<%end%>
<%end%>

</div>
